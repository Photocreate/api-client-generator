{
    "name": "photocreate/api-client-generator",
    "version": "0.1.0",
    "description": "This is a toolset to generate API client from Swagger specification.",
    "license": "MIT",
    "authors": [
        {
            "name": "Motohiro Imura",
            "email": "imunew@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Photocreate\\Api\\Client\\": "src/"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "twig/twig": "1.*",
        "symfony/console": "~2.8",
        "pimple/pimple": "^3.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
